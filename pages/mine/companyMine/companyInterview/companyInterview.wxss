page {
  margin-left: 5%;
  width: 90%;
  font-size: 10pt;
  color: #333;
}

.interview-item {
  margin-top: 20rpx;
  padding-bottom: 30rpx;
  border-bottom: rgba(246, 181, 1, 0.25) solid 2rpx;
}

.name {
  font-size: 11pt;
  font-weight: 600;
  line-height: 2.2em;
}

.item {
  margin-top: 10rpx;
  align-items: flex-start;
}

.title {
  width: 25%;
}

.title-data {
  width: 75%;
}

.outdate {
  color: #ccc;
}

.status {
  margin-left: 10rpx;
  color: white;
  font-size: 8pt;
  font-weight: 600;
  line-height: 1em;
  text-align: center;
  border-radius: 40rpx;
  width: 85rpx;
  padding: 5rpx 10rpx;
}

.three {
  background-color: #f6b501;
}

.four {
  background-color: turquoise;
}

.five {
  background-color: tomato;
}